    "node-cmd": "^3.0.0",
    "on-build-webpack": "^0.1.0",
    "postcss-import": "^12.0.0",
    "tailwindcss": "^0.6.1",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "webpack-watch": "^0.2.0",
    "yargs": "^4.6.0"
  },
